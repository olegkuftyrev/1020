{
  "name": "panda-express-dashboard-backend",
  "version": "1.0.0",
  "type": "module",
  "imports": {
    "#app/*": "./build/app/*.js",
    "#config/*": "./build/config/*.js",
    "#start/*": "./build/start/*.js",
    "#contracts/*": "./build/contracts/*.js",
    "#controllers/*": "./build/app/controllers/*.js",
    "#exceptions/*": "./build/app/exceptions/*.js",
    "#middleware/*": "./build/app/middleware/*.js",
    "#models/*": "./build/app/models/*.js",
    "#services/*": "./build/app/services/*.js",
    "#validators/*": "./build/app/validators/*.js"
  },
  "scripts": {
    "dev": "node --import tsx server.ts",
    "build": "tsc",
    "start": "node build/server.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@adonisjs/core": "^6.19.1",
    "@prisma/client": "^5.19.0",
    "prisma": "^5.19.0",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
